{
  "name": "cms",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:unocss": "unocss --out-file \"src/app/(payload)/uno.css\" \"./src/**/*.tsx\" --watch",
    "dev:next": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev": "run-p dev:unocss dev:next",
    "build:unocss": "unocss --out-file \"src/app/(payload)/uno.css\" \"./src/**/*.tsx\"",
    "build:next": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build": "pnpm generate:types && pnpm build:unocss && pnpm build:next",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "typecheck": "tsc",
    "migrate:v2": "cross-env tsx src/scripts/migrationV2/index.ts",
    "migrate:slate": "cross-env tsx src/scripts/migrationV2/slateToLexical.ts"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.0.0-beta.115",
    "@payloadcms/next": "3.0.0-beta.115",
    "@payloadcms/plugin-seo": "3.0.0-beta.115",
    "@payloadcms/richtext-lexical": "3.0.0-beta.115",
    "@payloadcms/storage-s3": "3.0.0-beta.115",
    "@payloadcms/translations": "3.0.0-beta.115",
    "@payloadcms/ui": "3.0.0-beta.115",
    "@react-email/components": "^0.0.25",
    "@types/escape-html": "^1.0.4",
    "axios": "^1.7.7",
    "cookie-es": "^1.2.2",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "escape-html": "^1.0.3",
    "next": "15.0.0-canary.173",
    "payload": "3.0.0-beta.115",
    "react": "19.0.0-rc-3edc000d-20240926",
    "react-dom": "19.0.0-rc-3edc000d-20240926",
    "react-email": "^3.0.1",
    "react-i18next": "^15.0.3",
    "sharp": "0.32.6",
    "slate": "^0.103.0",
    "slate-react": "^0.110.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/node": "^22.5.4",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@unocss/cli": "^0.63.4",
    "dotenv": "^16.4.5",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.19.1",
    "unocss": "^0.63.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
