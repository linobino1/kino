{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm generate:types && pnpm build:remix && pnpm build:copyfiles && pnpm build:payload && pnpm build:server",
    "build:copyfiles": "copyfiles -u 1 \"cms/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/cms/",
    "build:payload": "payload build",
    "build:remix": "remix build",
    "build:server": "tsc -p tsconfig.server.json",
    "dev": "remix dev -c 'node -r ts-node/register --watch-path ./server.ts server.ts'",
    "payload": "payload",
    "start": "pnpm payload migrate && NODE_ENV=production node dist/server.js",
    "typecheck": "tsc -p tsconfig.server.json",
    "generate:types": "cross-env payload generate:types",
    "seed": "ts-node ./cms/seed/index.ts"
  },
